{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "dev": "yarn prisma:deploy && concurrently \"PORT=8000 remix dev\" \"yarn build:tailwind --watch\"",
    "production": "yarn prisma:deploy && PORT=8000 remix-serve build",
    "build:all": "yarn build:tailwind && remix build && yarn prisma:generate",
    "build:tailwind": "npx tailwindcss -i styles/tailwind.css -o app/tailwind.css",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "postinstall": "remix setup node",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "@mdx-js/mdx": "^2.1.1",
    "@prisma/client": "^3.9.1",
    "@remix-run/react": "^1.1.3",
    "@remix-run/serve": "^1.1.3",
    "@types/mdx": "^2.0.1",
    "fuse.js": "^3.6.1",
    "intl-parse-accept-language": "^1.0.0",
    "prisma": "^3.9.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.3",
    "remix-auth": "^3.2.1",
    "remix-auth-google": "^1.1.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^7.0.0",
    "tailwindcss": "^3.0.17",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "version": "0.0.0"
}
