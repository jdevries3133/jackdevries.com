{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "cypress": "cypress run",
    "cypress:dev": "cypress open",
    "dev": "yarn prisma:deploy && concurrently \"PORT=8000 remix dev\" \"yarn build:tailwind --watch\"",
    "debug": "node --inspect-brk=0.0.0.0 ./node_modules/.bin/remix dev",
    "production": "yarn prisma:deploy && PORT=8000 remix-serve build",
    "build:all": "yarn build:tailwind && yarn prisma:generate && remix build",
    "build:tailwind": "tailwindcss -i styles/tailwind.css -o app/tailwind.css",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "postinstall": "remix setup node",
    "test": "vitest",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "@mdx-js/mdx": "^2.1.1",
    "@prisma/client": "^3.9.1",
    "@remix-run/dev": "^1.1.3",
    "@remix-run/react": "^1.1.3",
    "@remix-run/serve": "^1.1.3",
    "accept-language-parser": "^1.5.0",
    "concurrently": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "prisma": "^3.9.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rehype-highlight": "^5.0.2",
    "remix": "^1.1.3"
  },
  "devDependencies": {
    "@mdx-js/rollup": "^2.1.1",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "@types/accept-language-parser": "^1.5.3",
    "@types/mdx": "^2.0.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@vitejs/plugin-react": "^1.3.2",
    "cypress": "^9.7.0",
    "happy-dom": "^4.0.1",
    "tailwindcss": "^3.0.17",
    "@testing-library/jest-dom": "^5.16.4",
    "typescript": "^4.1.2",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.12.9"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "version": "0.0.0"
}
