[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=www-data
Group=www-data
WorkingDirectory=/home/jack/jackdevries.com/api
ExecStart=/home/jack/jackdevries.com/api/venv/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/jack/jackdevries.com/api/gunicorn.sock api.wsgi:application

[Install]
WantedBy=multi-user.target
