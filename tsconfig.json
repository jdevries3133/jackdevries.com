{
  "files": [],
  "references": [
    {
      "path": "./app"
    },
    {
      "path": "./cypress"
    }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "target": "ES2019",
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "lib": ["DOM", "DOM.Iterable", "ES2019"],
    "paths": {
      "~/*": ["app/*"]
    },
    "noEmit": true
  },
  "include": ["remix.env.d.ts", "**/*.ts", "**/*.tsx"]
}
